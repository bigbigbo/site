{
  "extends": ["tslint-config-airbnb", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "variable-name": false,
    "import-name": false,
    "function-name": false,
    "no-boolean-literal-compare": false
  }
}
